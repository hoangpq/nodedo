{
  "name": "react-hot",
  "version": "1.0.0",
  "description": "React hot with Relay and GraphQL",
  "scripts": {
    "start": "babel-node ./data/updateSchema.js && node server.js",
    "lint": "eslint src",
    "update-schema": "babel-node ./data/updateSchema.js",
    "prod:build": "babel . --out-dir ../build --ignore ./node_modules/ --copy-files",
    "prod:run": "cd ../build && yarn && node server.js"
  },
  "repository": {
    "type": "git"
  },
  "keywords": [
    "react",
    "reactjs",
    "boilerplate",
    "hot",
    "reload",
    "hmr",
    "live",
    "edit",
    "webpack"
  ],
  "author": "hoangpq",
  "license": "MIT",
  "devDependencies": {
    "babel-core": "^6.0.20",
    "babel-eslint": "^4.1.3",
    "babel-loader": "^6.0.1",
    "babel-preset-es2015": "^6.0.15",
    "babel-preset-react": "^6.0.15",
    "babel-preset-stage-0": "^6.0.15",
    "css-loader": "^0.28.4",
    "eslint": "^1.10.3",
    "eslint-plugin-react": "^3.6.2",
    "express": "^4.13.4",
    "graphql-tag": "latest",
    "react-apollo": "^1.4.2",
    "react-hot-loader": "^1.3.0",
    "sass-loader": "^6.0.5",
    "style-loader": "^0.18.2",
    "webpack-dev-middleware": "^1.6.1",
    "webpack-hot-middleware": "^2.10.0"
  },
  "dependencies": {
    "apollo-client": "^1.4.2",
    "axios": "^0.16.2",
    "babel-preset-flow": "^6.23.0",
    "babel-relay-plugin": "^0.11.0",
    "babel-relay-plugin-loader": "^0.11.0",
    "body-parser": "^1.17.2",
    "express-graphql": "^0.6.6",
    "graphql": "^0.10.1",
    "graphql-server-express": "^0.8.2",
    "graphql-tools": "^1.0.0",
    "knex": "^0.13.0",
    "mobx-react": "^4.2.1",
    "node-sass": "^4.5.3",
    "pg": "^6.2.4",
    "react": "^0.14.6",
    "react-dom": "^0.14.6",
    "react-relay": "^1.0.0",
    "react-router-dom": "^4.1.1",
    "relay": "^0.8.0-1",
    "sync-request": "^4.1.0",
    "webpack": "^2.6.1"
  }
